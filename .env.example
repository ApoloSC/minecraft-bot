# Configuración del Bot AFK de Minecraft

# Nombre de usuario del bot (para servidores no premium)
BOT_USERNAME=AFKBot

# Dirección del servidor
SERVER_HOST=server.dispearson.tech
SERVER_PORT=25565

# Tiempo de espera antes de reconectar (en milisegundos)
RECONNECT_DELAY=5000

# Intervalo de acciones anti-AFK (en milisegundos)
# 30000 = 30 segundos
ANTI_AFK_INTERVAL=30000